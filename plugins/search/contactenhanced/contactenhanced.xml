        <option value="0">JOFF</option><option value="1">JON</option></field><field name="search_archived" type="radio" class="btn-group" default="0" description="JFIELD_PLG_SEARCH_ARCHIVED_DESC" label="JFIELD_PLG_SEARCH_ARCHIVED_LABEL">
          
          
        <option value="0">JOFF</option><option value="1">JON</option></field></fieldset></fields>
