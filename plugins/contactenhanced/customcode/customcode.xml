  <description>PLG_CONTACTENHANCED_CUSTOMCODE_XML_DESCRIPTION</description>
  <files folder="site">
    <folder>code</folder>
    <folder>language</folder>
    <filename plugin="customcode">customcode.php</filename>
    <filename>index.html</filename>
